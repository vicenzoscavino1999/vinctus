{
  "name": "vinctus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "build:report": "node scripts/phase6/build-report.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:cycles": "madge --circular --extensions ts,tsx --ts-config tsconfig.json src",
    "check:stubs": "node scripts/check-stubs.mjs",
    "check:raw-onsnapshot": "node scripts/check-raw-onsnapshot.mjs",
    "guardrails": "npm run lint && npm run check:cycles && npm run check:stubs && npm run check:raw-onsnapshot",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage:unit": "vitest run --coverage",
    "test:coverage": "npm run test:coverage:unit && npm run test:emulators:coverage",
    "test:coverage:gate:p0": "npm run test:emulators:coverage && node scripts/phase6/assert-coverage.mjs",
    "test:emulators:run": "vitest run --config vitest.emulators.config.ts",
    "test:emulators:coverage:run": "vitest run --config vitest.emulators.config.ts --coverage",
    "test:emulators": "firebase emulators:exec --project vinctus-dev --only auth,firestore,storage,functions \"node scripts/run-emulator-tests.mjs\"",
    "test:emulators:coverage": "firebase emulators:exec --project vinctus-dev --only auth,firestore,storage,functions \"node scripts/run-emulator-tests.mjs --coverage\"",
    "test:rules:run": "vitest run --config vitest.rules.config.ts",
    "test:rules": "firebase emulators:exec --project vinctus-dev --only auth,firestore,storage \"npm run test:rules:run\"",
    "test:e2e": "playwright test",
    "test:e2e:critical:run": "playwright test e2e/critical-flows.spec.ts --workers=1",
    "test:e2e:critical": "node scripts/run-critical-e2e.mjs",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "type:coverage": "type-coverage --detail --strict --tsconfig tsconfig.json",
    "validate": "npm run typecheck && npm run guardrails && npm run test:coverage && vite build",
    "ci": "npm run lint && npm run build && npm run test:run",
    "phase6:baseline": "node scripts/phase6/capture-baseline.mjs",
    "phase6:inventory": "node scripts/phase6/inventory-api-modules.mjs --out docs/phase6/baseline/api-modules.json",
    "phase6:day14": "node scripts/phase6/day14-report.mjs",
    "phase6:day15": "npm run validate && npm run test:e2e:critical",
    "lhci:phase6": "node scripts/phase6/lhci-baseline.mjs",
    "emulators": "firebase emulators:start --only auth,firestore,storage,functions",
    "seed": "node scripts/seed.mjs",
    "dev:local": "node scripts/dev-local.mjs",
    "functions:install": "npm --prefix functions install",
    "functions:build": "npm --prefix functions run build",
    "prepare": "simple-git-hooks"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "browser-image-compression": "^2.0.2",
    "firebase": "^12.7.0",
    "heic2any": "^0.0.4",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/node": "^5.5.28",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "firebase-admin": "^13.6.0",
    "firebase-tools": "^13.16.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.1.0",
    "lint-staged": "^15.2.10",
    "madge": "^8.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.17",
    "type-coverage": "^2.29.7",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16",
    "wait-on": "^7.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md,json,yml,yaml}": "prettier --write"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
