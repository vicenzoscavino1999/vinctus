{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:4173/",
        "http://localhost:4173/feed",
        "http://localhost:4173/messages",
        "http://localhost:4173/group/lhci-group"
      ],
      "numberOfRuns": 3,
      "startServerCommand": "npm run preview -- --port 4173 --strictPort",
      "startServerReadyPattern": "http://localhost:4173/",
      "settings": {
        "preset": "desktop",
        "chromeFlags": "--no-sandbox --disable-dev-shm-usage"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.9 }]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "docs/phase6/reports/lhci"
    }
  }
}
